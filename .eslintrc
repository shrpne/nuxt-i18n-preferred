{
  "extends": "eslint-config-airbnb-base",
  "rules": {
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "max-len": ["off"],
    "no-use-before-define": ["off"],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-else-return": 0,
    "no-param-reassign": 0
  },
  "env": {
    "browser": true
  }
}
